#include <stdio.h>
#include <stdlib.h>

int main() {
    int a , b, c;

    printf("number A : ");
    scanf("%d" , &a);

    printf("number B : ");
    scanf("%d" , &b);

    printf("number C : ");
    scanf("%d" , &c);

    if(a > b && b > c){
        printf("first number : %d\n second number : %d\n third number : %d\n", a , b ,c);
    }
    else if(a > c && c > b){
        printf("first number : %d\n second number : %d\n third number : %d\n", a , c ,b);
    }
    else if (b > a && a > c){
        printf("first number : %d\n second number : %d\n third number : %d\n", b , a ,c);
    }
    else if(b > c && c > a){
        printf("first number : %d\n second number : %d\n third number : %d\n", b , c ,a);
    }
    else if(c > a && a > b){
        printf("first number : %d\n second number : %d\n third number : %d\n", c , a ,b);
    }
    else if(c > b && b > a){
        printf("first number : %d\n second number : %d\n third number : %d\n", c , b ,a);
    }







return 0;
}
