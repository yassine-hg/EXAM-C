#include <stdio.h>
#include <stdlib.h>

int main() {
    int a , b , c , max ;

    printf("enter la valeur de A : ");
    scanf("%d" , &a);

    printf("enter la valeur de B : ");
    scanf("%d" , &b);

    printf("enter la valeur de C : ");
    scanf("%d" , &c);

    max = a;
    if (max < b){
        max=b;
    }
    if (max < c){
        max = c;
    }
    printf("la valeur maximal est %d" , max);









return 0;
}
